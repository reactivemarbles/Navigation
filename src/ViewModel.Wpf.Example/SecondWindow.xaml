<rxNav:NavigationWindow
    x:Class="ViewModel.Wpf.Example.SecondWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ViewModel.Wpf.Example"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:rxNav="https://github.com/reactivemarbles/Navigation"
    x:Name="secondWindow"
    Title="SecondWindow"
    Width="800"
    Height="450"
    Style="{DynamicResource NavigationWindowStyle}"
    mc:Ignorable="d">
    <Grid>
        <Button
            x:Name="NavBack"
            Height="30"
            Content="Back" />
    </Grid>
</rxNav:NavigationWindow>
